    }
  ],
  "scripts": {
    "test": "phpunit"
  },
  "require": {
    "php": "^7.2|^8",
    "ext-dom": "*",
    "masterminds/html5": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit" : "^6|^7|^8",
    "d11wtq/boris": "1.0.8"
