{
  "name": "hametuha/hamepub",
  "description": "PHP Library to handle ePub 3.0",
  "type": "library",
  "version": "1.0.0",
  "keywords": ["WordPress", "ePub"],
  "homepage": "https://github.com/hametuha/hamepub",
  "license": "MIT",
  "authors": [
    {
      "name": "Takahashi Fumiki",
